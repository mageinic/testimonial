<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="MageINIC_Core::core">
                    <resource id="MageINIC_Testimonial::testimonial" sortOrder="10"
                              title="Testimonial" translate="title">
                        <resource id="MageINIC_Testimonial::manage_testimonial" sortOrder="10"
                                  title="Testimonial" translate="title">
                            <resource id="MageINIC_Testimonial::save" sortOrder="10"
                                      title="Save Testimonial" translate="title"/>
                            <resource id="MageINIC_Testimonial::delete" sortOrder="20"
                                      title="Delete Testimonial" translate="title"/>
                        </resource>
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="MageINIC_Core::core_config">
                                <resource id="MageINIC_Testimonial::testimonial_config" title="Testimonial"
                                          translate="title"/>
                            </resource>
                        </resource>
                    </resource>
                </resource>
            </resource>
        </resources>
    </acl>
</config>
